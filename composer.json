{
    "name": "matthiasnoback/broadway-serialization",
    "type": "library",
    "description": "Serialization helpers for Broadway",
    "keywords": ["Broadway", "serialization"],
    "homepage": "http://github.com/matthiasnoback/broadway-serialization",
    "license": "MIT",
    "authors": [
        {
            "name": "Matthias Noback",
            "email": "matthiasnoback@gmail.com",
            "homepage": "http://php-and-symfony.matthiasnoback.nl"
        }
    ],
    "require": {
        "php": "^7.0|^8.0",
        "doctrine/instantiator": "^1.0",
        "broadway/broadway": "^1.0|^2.1"
    },
    "autoload": {
        "psr-4": {
            "BroadwaySerialization\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BroadwaySerialization\\Test\\": "test/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "phpbench/phpbench": "^1.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
